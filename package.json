{
  "name": "nuxt-app",
  "version": "1.0.0",
  "module": "commonjs",
  "private": true,
  "scripts": {
    "build": "pnpm prisma generate && nuxt build",
    "build:sqlite": "pnpm prisma:files && nuxt build && pnpm prisma:db",
    "dev": "concurrently \"nuxt dev --host\" \"sass --watch scss/style.scss:assets/css/style.css\"",
    "sass:watch": "sass --watch scss/style.scss:assets/css/style.css &",
    "prisma:files": "pnpm prisma generate && cp ./node_modules/.prisma/client/*.js ./node_modules/@prisma/client",
    "prisma:files:windows": "pnpm prisma generate && xcopy /s /y .\\\\node_modules\\\\.prisma\\\\client\\\\*.js .\\\\node_modules\\\\@prisma\\\\client &",
    "prisma:db": "cp ./prisma/dev.db ./.output/server/node_modules/.prisma/client",
    "prisma:db:windows": "xcopy /s /y .\\\\prisma\\\\dev.db .\\\\.output\\\\server\\\\node_modules\\\\.prisma\\\\client &",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "pnpm run migrate && nuxt start",
    "studio": "pnpm prisma studio",
    "migrate": "pnpm prisma migrate dev --name init && pnpm prisma generate",
    "build:windows": "pnpm prisma:files:windows && nuxt build && pnpm prisma:db:windows"
  },
  "devDependencies": {
    "@types/multer": "^1.4.11",
    "@types/node": "^18.19.33",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "nuxt": "^3.11.2",
    "sass": "^1.77.0"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "h3-formidable": "^0.3.8",
    "nodemailer": "^6.9.13",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "unstorage": "^1.10.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "ws": "^8.17.0"
  }
}
